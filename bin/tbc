#!/bin/bash

source ~/bin/.functions.sh

function getinfo {
    os_info
    echo
    system_info
}

case "$1" in
    cmd)
        echo 'cmd'
        ;;
    info)
        getinfo
        ;;
    install)
        shift
        package_install "$*"
        ;;
    remove)
        shift
        package_remove "$*"
        ;;
    bench)
        benchmark
        ;;
    update)
        system_update
        command_update
        ;;
    *)
        echo 'syntax'
        ;;
esac
